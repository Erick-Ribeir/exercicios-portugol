programa
{
	
	funcao inicio()
	{
		escreva("##########----- EXERCÍCIO 6 -----##########\n")
		inteiro numero, menor
		escreva ("Entre com o primeiro número: ")
		leia(numero)
		menor = numero
		
			faca{
				escreva("entre com outro número [0-sair]: ")
				leia(numero)
				se(numero < menor e numero!=0)
					menor = numero
					
				
					
				}enquanto(numero!=0)
					escreva("Menor valor: ", menor)

		escreva("##########----- FIM -----##########\n")
		
		
	}
}

